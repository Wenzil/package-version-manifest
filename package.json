{
    "name": "package-version-manifest",
    "version": "1.0.2",
    "description": "Simple CLI to generate package version manifests",
    "keywords": [
        "package",
        "semver",
        "version",
        "manifest"
    ],
    "author": {
        "name": "Sami Turcotte",
        "email": "samiturcotte@gmail.com"
    },
    "contributors": [
        {
            "name": "Sami Turcotte",
            "email": "samiturcotte@gmail.com"
        }
    ],
    "license": "MIT",
    "bin": {
      "package-version-manifest": "./dist/index.js"
    },
    "types": "dist/index.d.ts",
    "files": ["dist"],
    "repository": {
        "url": "git@github.com:Wenzil/package-version-manifest.git",
        "type": "git"
    },
    "scripts": {
        "test": "ava",
        "lint": "tslint -p tsconfig.json",
        "validate:tslint": "tslint-config-prettier-check ./tslint.json",
        "prepublish": "yarn lint && yarn test && yarn tsc"
    },
    "dependencies": {
        "semver": "^5.6.0"
    },
    "devDependencies": {
        "@types/node": "^10.10.1",
        "@types/semver": "^5.5.0",
        "ava": "^1.0.0-rc.2",
        "prettier": "^1.14.3",
        "ts-node": "^7.0.1",
        "tslint": "^5.11.0",
        "tslint-config-prettier": "^1.16.0",
        "tslint-plugin-prettier": "^2.0.1",
        "typescript": "^3.1.6"
    },
    "ava": {
        "files": [
            "src/**/*.spec.ts"
        ],
        "sources": [
            "src/**/*.ts"
        ],
        "compileEnhancements": false,
        "extensions": [
            "ts"
        ],
        "require": [
            "ts-node/register/transpile-only"
        ]
    }
}
